<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core" >
 <TableSelectDialog id="_IDGenTableSelectDialog1.1"
		noDataText="No Data"
		title="Freight Forwarder Selection"
		search=".handleSearch"
    confirm=".handleValueHelpClose"
		cancel=".handleValueHelpClose"
		items="{/Customer}">
		<ColumnListItem id="_IDGenColumnListItem1.1" selected="{selected}">
			<cells>
					<ObjectIdentifier id="_IDGenObjectIdentifier12" title="{name}"  />
                            <Text id="create_text_address" text="{address}"/>
                            <Text id="create_text_phone" text="{phone}" visible="false"/>
                            <Text id="create_text_jobLocation" text="{jobLocation}" visible="false"/>

				
			</cells>
		</ColumnListItem>
		<columns>
			<Column id="_IDGenColumn12" width="12em">
				<header>
					<Text id="_IDGenText7.1" text="{i18n>cusatomername}" />
				</header>
			</Column>
			<Column id="_IDGenColumn13" width="12em">
				<header>
					<Text id="_IDGenText8.1" text="{i18n>customerAddress}" />
				</header>
			</Column>

			<Column id="_IDGenColumn15" width="12em">
				<header>
					<Text id="_IDGenText9.1" text="{i18n>customerphone}" visible="false"/>
				</header>
			</Column>

			<Column id="_IDGenColumn16" width="12em">
				<header>
					<Text id="_IDGenText10.1" text="{i18n>jobLocation}" visible="false"/>
				</header>
			</Column>
			
			
			
		</columns>
	</TableSelectDialog>
         

</core:FragmentDefinition>